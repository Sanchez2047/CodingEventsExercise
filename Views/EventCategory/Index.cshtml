@using CodingEvents.Models

@model List<EventCategory>

<h1>Event Categories</h1>


<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    @foreach (var cat in Model)
    {
        <tr>
            <td>@cat.Id</td>
            <td>@cat.Name</td>
        </tr>
    }
</table>

